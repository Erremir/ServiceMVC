@model IEnumerable<ServiceMVC.Models.ArancelUnidad>

@{
    ViewData["Title"] = "Arancel Unidad de Trabajo";
}

<h1>Arancel Unidad de Trabajo</h1>

<p>
    <a asp-action="Create">Nuevo</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Precio)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Precio)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ArancelUnidadID">Editar</a> |
                <a asp-action="Details" asp-route-id="@item.ArancelUnidadID">Detalles</a> |
                <a asp-action="Delete" asp-route-id="@item.ArancelUnidadID">Eliminar</a>
            </td>
        </tr>
}
    </tbody>
</table>
